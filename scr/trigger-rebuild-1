body='{
"request": {
  "branch":"master"
}}'

# TRAVIS LIMITS TO 10 BUILDS AT A TIME
for repo in \
    chalk_line guerrero hamon_shu magnolia \
    onkos passagenwerk sim
do
    curl -s -X POST \
    -H "Content-Type: application/json" \
    -H "Accept: application/json" \
    -H "Travis-API-Version: 3" \
    -H "Authorization: token 5VzHyHPj2-Gx1pGRQ0ppQg" \
    -d "$body" \
    https://api.travis-ci.com/repo/GregoryREvans%2F$repo/requests
done
